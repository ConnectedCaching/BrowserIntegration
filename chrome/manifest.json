{
	
	"manifest_version": 2,
	
	"name": "Connected Caching",
	"description": "FooBar",
	"version": "0.1",
	
	"background": {
		"scripts": ["background.js"] 
	},
	
	"page_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	
	"permissions": [
		"tabs",
		"http://www.opencaching.com/*/#!geocache/*"
	]

}